{
  "name": "editor",
  "private": true,
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/css/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile src/css/smui-dark.css -i src/theme/dark",
    "deploy": "vite build --base=./ && rsync -av --delete dist/ siev20@ssh.student.bth.se:www/editor",
    "test": "npx vitest run"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@smui/select": "^6.1.0",
    "@smui/snackbar": "^6.1.0",
    "@smui/textfield": "^6.1.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/svelte": "^3.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@tinymce/tinymce-svelte": "^1.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/tinymce": "^4.6.5",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/ui": "^0.23.4",
    "eslint": "^8.23.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "smui-theme": "^6.1.3",
    "socket.io-client": "^4.5.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-fa": "^3.0.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  }
}
